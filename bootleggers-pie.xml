<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="gitlab"
           fetch="https://gitlab.com" />

  <remote  name="Galaxy-MSM8916"
           fetch="https://github.com"
           review="review.msm8916.com"
           revision="lineage-16.0" />

  <remove-project name="platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9" />

<!-- Samsung Hardware Repo -->
  <project path="hardware/samsung" name="jenslody/android_hardware_samsung.git" remote="gitlab" revision="lineage-16.0" />
 
 <!-- Samsung MSM8916 Common Repos -->
  <project path="device/samsung/qcom-common" name="jenslody/android_device_samsung_qcom-common.git" remote="gitlab" groups="device" revision="lineage-16.0" />
  <project path="device/samsung/msm8916-common" name="jenslody/android_device_samsung_msm8916-common.git" groups="device" remote="gitlab" revision="lineage-16.0" />

 <!-- Galaxy J5 (SM-J500H - j53gxx) || (J500F-G-M-Y - j5lte) || (J500FN - j5nlte) || (J5008 - j5ltechn) -->
  <project path="device/samsung/j5nlte" name="jenslody/android_device_samsung_j5nlte.git" groups="device" remote="gitlab" revision="pie" />
  <project path="device/samsung/j5ltechn" name="jenslody/android_device_samsung_j5ltechn.git" groups="device" remote="gitlab" revision="pie" />
  <project path="device/samsung/j5lte" name="jenslody/android_device_samsung_j5lte.git" groups="device" remote="gitlab" revision="pie" />
  <project path="device/samsung/j53gxx" name="jenslody/android_device_samsung_j53gxx.git" groups="device" remote="gitlab" revision="pie" />
  <project path="device/samsung/j5-common" name="jenslody/android_device_samsung_j5-common.git" groups="device" remote="gitlab" revision="lineage-16.0" />

 <!-- (J510FN - j5xnlte) -->
  <project path="device/samsung/j5x-common" name="jenslody/android_device_samsung_j5x-common.git" groups="device" remote="gitlab" revision="lineage-16.0" />
  <project path="device/samsung/j5xnlte" name="jenslody/android_device_samsung_j5xnlte.git" groups="device" remote="gitlab" revision="pie" />


 <!-- arm-eabi-7.2 -->
  <project path="prebuilts/gcc/linux-x86/arm/arm-eabi-7.2" name="Galaxy-MSM8916/android-prebuilts-gcc-linux-x86-arm-arm-eabi-7.2" groups="pdk,linux,arm"  remote="github" revision="master"/>

